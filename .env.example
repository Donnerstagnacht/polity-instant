# InstantDB Configuration
NEXT_PUBLIC_INSTANT_APP_ID=your_instant_app_id_here
INSTANT_ADMIN_TOKEN=your_instant_admin_token_here

# Stripe Payment Integration (Optional)
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# OpenAI Integration (Optional)
OPENAI_API_KEY=your_openai_api_key_here

# Application URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Web Push Notifications (VAPID Keys)
# Generate keys by running: node scripts/generate-vapid-keys.ts
VAPID_PUBLIC_KEY="your_vapid_public_key_here"
VAPID_PRIVATE_KEY="your_vapid_private_key_here"
VAPID_EMAIL="mailto:your-email@example.com"
NEXT_PUBLIC_VAPID_PUBLIC_KEY="your_vapid_public_key_here"  # Same as VAPID_PUBLIC_KEY
